<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav2"
    app:startDestination="@id/mainHomeFragment">

    <fragment
        android:id="@+id/mainHomeFragment"
        android:name="com.example.mannayoclient.MainHomeFragment"
        android:label="MainHomeFragment" >
        <action
            android:id="@+id/action_mainHomeFragment_to_categoryListFragment"
            app:destination="@id/categoryListFragment" />
        <action
            android:id="@+id/action_mainHomeFragment_to_favoritistFragment"
            app:destination="@id/favoritistFragment" />


        <action
            android:id="@+id/action_mainHomeFragment_to_advertiseFragment"
            app:destination="@id/advertiseFragment" />
        <action
            android:id="@+id/action_mainHomeFragment_to_todayFragment"
            app:destination="@id/todayFragment" />
        <action
            android:id="@+id/action_mainHomeFragment_to_alarmFragment"
            app:destination="@id/alarmFragment" />

    </fragment>
    <fragment
        android:id="@+id/categoryListFragment"
        android:name="com.example.mannayoclient.categorylist.CategoryListFragment"
        android:label="CategoryListFragment" >
        <action
            android:id="@+id/action_categoryListFragment_to_mainStoreFragment"
            app:destination="@id/mainStoreFragment" />
        <action
            android:id="@+id/action_categoryListFragment_self"
            app:destination="@id/categoryListFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainStoreFragment"
        android:name="com.example.mannayoclient.mainmenulist.MainStoreFragment"
        android:label="MainStoreFragment" >
        <action
            android:id="@+id/action_mainStoreFragment_to_storeMenuFragment"
            app:destination="@id/storeMenuFragment" />
        <action
            android:id="@+id/action_mainStoreFragment_to_storeReviewFragment"
            app:destination="@id/storeReviewFragment" />
    </fragment>
    <fragment
        android:id="@+id/storeMenuFragment"
        android:name="com.example.mannayoclient.storemenulist.StoreMenuFragment"
        android:label="StoreMenuFragment" />
    <fragment
        android:id="@+id/favoritistFragment"
        android:name="com.example.mannayoclient.favoritist.favoritistFragment"
        android:label="favoritistFragment" >
        <action
            android:id="@+id/action_favoritistFragment_to_mainStoreFragment"
            app:destination="@id/mainStoreFragment" />
    </fragment>
    <fragment
        android:id="@+id/storeReviewFragment"
        android:name="com.example.mannayoclient.storereviewlist.StoreReviewFragment"
        android:label="StoreReviewFragment" >
        <action
            android:id="@+id/action_storeReviewFragment_to_reviewWriteFragment"
            app:destination="@id/reviewWriteFragment" />
    </fragment>
    <fragment
        android:id="@+id/reviewWriteFragment"
        android:name="com.example.mannayoclient.ReviewWriteFragment"
        android:label="ReviewWriteFragment" />
    <fragment
        android:id="@+id/advertiseFragment"
        android:name="com.example.mannayoclient.advertiselist.AdvertiseFragment"
        android:label="AdvertiseFragment" >
        <action
            android:id="@+id/action_advertiseFragment_to_writeFragment"
            app:destination="@id/writeFragment" />
    </fragment>
    <fragment
        android:id="@+id/writeFragment"
        android:name="com.example.mannayoclient.WriteFragment"
        android:label="WriteFragment" />
    <fragment
        android:id="@+id/todayFragment"
        android:name="com.example.mannayoclient.todaylist.TodayFragment"
        android:label="TodayFragment" >
        <action
            android:id="@+id/action_todayFragment_to_writeFragment"
            app:destination="@id/writeFragment" />
    </fragment>
    <fragment
        android:id="@+id/alarmFragment"
        android:name="com.example.mannayoclient.alarmlist.AlarmFragment"
        android:label="AlarmFragment" />
</navigation>