<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#ffffff"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView65"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/close_round" />

    <ImageView
        android:id="@+id/imageView67"
        android:layout_width="70dp"
        android:layout_height="22dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView65"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView65"
        app:srcCompat="@drawable/write" />

    <ImageView
        android:id="@+id/imageView68"
        android:layout_width="66dp"
        android:layout_height="33dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView67"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView67"
        app:srcCompat="@drawable/component_114" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="312dp"
        android:layout_height="48dp"
        android:layout_marginTop="50dp"
        android:background="#ffffff"
        android:ems="10"
        android:fontFamily="@font/noto_sans_kr"
        android:gravity="top"
        android:hint="내용을 입력하세요. (최대 20줄까지 작성 가능)"
        android:inputType="textMultiLine"
        android:textColor="#191919"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView67" />

    <ImageView
        android:id="@+id/imageView69"
        android:layout_width="110dp"
        android:layout_height="56dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/component_100" />

    <ImageView
        android:id="@+id/imageView70"
        android:layout_width="110dp"
        android:layout_height="56dp"
        android:layout_marginStart="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView69"
        app:layout_constraintStart_toEndOf="@+id/imageView69"
        app:srcCompat="@drawable/vote" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="312dp"
        android:layout_height="200dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@drawable/votebox"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName4">

        <com.example.mannayoclient.TextClear
            android:id="@+id/vote_title"
            android:layout_width="312dp"
            android:layout_height="48dp"
            android:background="@null"
            android:ems="10"
            android:fontFamily="@font/noto_sans_kr"
            android:hint="투표 제목"
            android:inputType="textEmailAddress"
            android:lineSpacingExtra="7sp"
            android:paddingLeft="12dp"
            android:textColor="#191919"
            android:textSize="13sp"
            android:textStyle="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view12"
            android:layout_width="312dp"
            android:layout_height="0.8dp"
            android:background="#dbdbdb"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/vote_title" />

        <com.example.mannayoclient.TextClear
            android:id="@+id/vote1"
            android:layout_width="288dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/votebox"
            android:ems="10"
            android:fontFamily="@font/noto_sans_kr"
            android:hint="투표 제목"
            android:inputType="textEmailAddress"
            android:paddingLeft="12dp"
            android:textStyle="normal"
            android:textSize="12sp"
            android:textColor="#191919"
            android:lineSpacingExtra="8sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/vote_title" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>