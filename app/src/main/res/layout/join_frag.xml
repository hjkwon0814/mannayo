<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="683dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="683dp"
    android:background="#ffffff"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/logo_image"
        android:layout_width="162dp"
        android:layout_height="28dp"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo"
       />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="114dp"
        android:layout_height="20dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo_image"
        app:srcCompat="@drawable/join1" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="117dp"
        android:layout_height="20dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="@+id/editTextTextEmailAddress"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:srcCompat="@drawable/join2"
      />

    <com.example.mannayoclient.TextClear
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="312dp"
        android:layout_height="52dp"
        android:background="#f1f1f5"
        android:ems="10"
        android:fontFamily="@font/noto_sans_kr"
        android:hint="아이디 또는 이메일을 입력해주세요."
        android:inputType="textEmailAddress"
        android:paddingLeft="12dp"
        android:textColor="#191919"
        android:textSize="14sp"
        android:textStyle="normal"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
       />

    <com.example.mannayoclient.TextClear
        android:id="@+id/editTextTextPassword"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="10dp"
        android:layout_width="312dp"
        android:layout_height="52dp"
        android:fontFamily="@font/noto_sans_kr"
        android:hint="8~12자내로 영문, 숫자, 특수문자 사용 가능합니다."
        android:paddingLeft="12dp"
        android:textColor="#191919"
        android:background="#f1f1f5"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
      />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_marginTop="20dp"
        android:layout_width="52dp"
        android:layout_height="20dp"
        app:layout_constraintStart_toStartOf="@+id/editTextTextEmailAddress"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress"
        app:srcCompat="@drawable/join3"
     />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="94dp"
        android:layout_height="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPassword"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword"
        app:srcCompat="@drawable/join4"
       />

    <com.example.mannayoclient.TextClear
        android:id="@+id/editTextTextPassword2"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="312dp"
        android:layout_height="52dp"
        android:fontFamily="@font/noto_sans_kr"
        android:hint="비밀번호를 다시 한번 입력해주세요."
        android:paddingLeft="12dp"
        android:textColor="#191919"
        android:textSize="14sp"
        android:background="#f1f1f5"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="26dp"
        android:layout_height="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPassword2"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2"
        app:srcCompat="@drawable/join5"
        />

    <com.example.mannayoclient.TextClear
        android:id="@+id/editTextTextPersonName"

        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        android:layout_width="312dp"
        android:layout_height="52dp"
        android:fontFamily="@font/noto_sans_kr"
        android:hint="이름을 입력해주세요."
        android:paddingLeft="12dp"
        android:textColor="#191919"
        android:textSize="14sp"
        android:background="#f1f1f5"
        android:textStyle="normal"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
         />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="52dp"
        android:layout_height="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPersonName"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName"
        app:srcCompat="@drawable/join6" />

    <EditText
        android:id="@+id/editTextDate"
        android:layout_width="98dp"
        android:layout_height="52dp"
        android:layout_marginTop="10dp"
        android:background="#f1f1f5"
        android:ems="10"
        android:fontFamily="@font/noto_sans_kr"
        android:hint="년(4자)"
        android:inputType="date"
        android:paddingLeft="12dp"
        android:textColor="#191919"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toBottomOf="@+id/imageView12" />

    <EditText
        android:id="@+id/editTextDate2"
        android:layout_width="98dp"
        android:layout_height="52dp"
        android:layout_marginStart="9dp"
        android:background="#f1f1f5"
        android:ems="10"
        android:fontFamily="@font/noto_sans_kr"
        android:hint="월"
        android:inputType="date"
        android:paddingLeft="12dp"
        android:textColor="#191919"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBaseline_toBaselineOf="@+id/editTextDate"
        app:layout_constraintStart_toEndOf="@+id/editTextDate" />

    <EditText
        android:id="@+id/editTextDate3"
        android:layout_width="98dp"
        android:layout_height="52dp"
        android:background="#f1f1f5"
        android:ems="10"
        android:fontFamily="@font/noto_sans_kr"
        android:hint="일"
        android:inputType="date"
        android:paddingLeft="12dp"
        android:textColor="#191919"
        android:textSize="14sp"
        android:layout_marginStart="9dp"
        android:textStyle="normal"
        app:layout_constraintBaseline_toBaselineOf="@+id/editTextDate2"
        app:layout_constraintStart_toEndOf="@+id/editTextDate2" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="52dp"
        android:layout_height="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@+id/editTextDate"
        app:layout_constraintTop_toBottomOf="@+id/editTextDate2"
        app:srcCompat="@drawable/join7" />

    <com.example.mannayoclient.TextClear
        android:id="@+id/editTextNumber"
        android:ems="10"
        android:inputType="number"
        android:layout_marginTop="10dp"
        android:layout_width="312dp"
        android:layout_height="52dp"
        android:fontFamily="@font/noto_sans_kr"
        android:hint="휴대폰번호를 입력해주세요.(‘-’제외)"
        android:paddingLeft="12dp"
        android:textColor="#191919"
        android:textSize="14sp"
        android:background="#f1f1f5"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/imageView14"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <ImageView
        android:id="@+id/join_submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="314dp"
        android:layout_height="57dp"
        android:layout_marginTop="40dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/component_5"
        tools:layout_editor_absoluteY="178dp"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumber" />


</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
