<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <stroke
                android:width="1dp"
                android:color="#ffa463" />
            <padding
                android:left="29dp"
                android:top="11dp"
                android:right="29dp"
                android:bottom="11dp"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <stroke
                android:width="1dp"
                android:color="#dbdbdb" />
            <padding
                android:left="29dp"
                android:top="11dp"
                android:right="29dp"
                android:bottom="11dp"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <stroke
                android:width="1dp"
                android:color="#dbdbdb" />
            <padding
                android:left="29dp"
                android:top="11dp"
                android:right="29dp"
                android:bottom="11dp"/>
        </shape>
    </item>

</selector>